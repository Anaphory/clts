[run]
branch = True
source = pyclts

[paths]
source =
    src/pyclts
    .tox/*/lib/python*/site-packages/pyclts

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    tests/*
show_missing = True

